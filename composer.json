{
    "name": "d2emonium/miniature-octolana",
    "description": "Miniature Octalana loto game",
    "keywords": ["loto", "game"],
    "homepage": "http://octolana.local/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/d2emonium/miniature-octolana/issues?state=open",
        "wiki": "https://github.com/d2emonium/miniature-octolana/issues?state=open",
        "source": "https://github.com/d2emonium/miniature-octolana/wiki"
    },
    "minimum-stability": "dev",
    "repositories":[
        {
            "type": "git",
	          "url":  "git@github.com:d2emon/game-collection.git"
        },
        {
            "type": "git",
	          "url":  "git@github.com:d2emon/book-collection.git"
        },
        {
            "type": "git",
	          "url":  "git@github.com:d2emon/trcards.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "philippfrenzel/yii2masonry": "*",
        "d2emon/book-collection": "*",
        "d2emon/game-collection": "*",
        "codeception/codeception": "2.0.*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "codeception/c3": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
